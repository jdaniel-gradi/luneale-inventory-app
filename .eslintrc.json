{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier", "eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error", { "singleQuote": true }],
        "quotes": ["error", "single", { "avoidEscape": true }],
        "no-console": "off"
    }
}
